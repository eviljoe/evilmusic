{
    "ecmaVersion": 6,
    "libs": [
        "browser"
    ],
    "loadEagerly": [
        "src/main/webapp/index.js",
        "src/main/webapp/assets/**/*.js",
        "src/main/webapp/components/**/*.js",
        "src/main/webapp/pipes/**/*.js",
        "src/main/webapp/services/**/*.js",
        "src/main/webapp/utils/**/*.js",
        "src/test/webapp/**/*.js"
    ],
    "plugins": {
        "es_modules": {},
        "complete_strings": {},
        "angular": {},
        "doc_comment": {
            "fullDocs": true
        }
    }
}
