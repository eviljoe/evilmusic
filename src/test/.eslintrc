{
    "extends": "../../.eslintrc",
     
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    
    "env": {
        "jasmine": true,
        "protractor": true,
        "phantomjs": true
    },
    
    "rules": {
        "max-statements": ["off"],
        "no-empty-function": ["off"],
        "no-magic-numbers": ["off"]
    }
}